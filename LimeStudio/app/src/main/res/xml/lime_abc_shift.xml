<Keyboard xmlns:limehd="http://schemas.android.com/apk/res-auto"
    limehd:keyWidth="10%p"
    limehd:horizontalGap="0px"
    limehd:verticalGap="0px"
    limehd:keyHeight="@dimen/key_height"
    >

	<Row>
		<Key limehd:codes="33" limehd:keyLabel="!"  limehd:keyEdgeFlags="left" />
		<Key limehd:codes="64" limehd:keyLabel="\@" />
		<Key limehd:codes="35" limehd:keyLabel="\#" />
		<Key limehd:codes="36" limehd:keyLabel="$" />
		<Key limehd:codes="37" limehd:keyLabel="%" />
		<Key limehd:codes="94" limehd:keyLabel="^" />
		<Key limehd:codes="38" limehd:keyLabel="&amp;" />
		<Key limehd:codes="42" limehd:keyLabel="*" />
		<Key limehd:codes="40" limehd:keyLabel="(" />
		<Key limehd:codes="41" limehd:keyLabel=")" limehd:keyEdgeFlags="right" />
	</Row>
    
   <Row>
        <Key limehd:codes="65" limehd:keyLabel="A" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="66" limehd:keyLabel="B"/>
        <Key limehd:codes="67" limehd:keyLabel="C"/>
        <Key limehd:codes="68" limehd:keyLabel="D"/>
        <Key limehd:codes="69" limehd:keyLabel="E"/>
        <Key limehd:codes="70" limehd:keyLabel="F"/>
        <Key limehd:codes="71" limehd:keyLabel="G"/>
        <Key limehd:codes="72" limehd:keyLabel="H"/>
        <Key limehd:codes="73" limehd:keyLabel="I"/>
        <Key limehd:codes="74" limehd:keyLabel="J" limehd:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key limehd:codes="75" limehd:keyLabel="K"  limehd:horizontalGap="5%p"  limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="76" limehd:keyLabel="L" />
        <Key limehd:codes="77" limehd:keyLabel="M"/>
        <Key limehd:codes="78" limehd:keyLabel="N"/>
        <Key limehd:codes="79" limehd:keyLabel="O"/>
        <Key limehd:codes="80" limehd:keyLabel="P" />
        <Key limehd:codes="81" limehd:keyLabel="Q" />
        <Key limehd:codes="82" limehd:keyLabel="R"/>
        <Key limehd:codes="83" limehd:keyLabel="S"  limehd:keyEdgeFlags="right"/>
    </Row>
    <Row>
        <Key limehd:codes="-1" limehd:keyIcon="@drawable/sym_keyboard_shift_shifted"
                limehd:keyWidth="15%p" limehd:isModifier="true"
                limehd:isSticky="true" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="84" limehd:keyLabel="T"/>
        <Key limehd:codes="85" limehd:keyLabel="U"/>
        <Key limehd:codes="86" limehd:keyLabel="V"/>
        <Key limehd:codes="87" limehd:keyLabel="W"/>
        <Key limehd:codes="88" limehd:keyLabel="X"/>
        <Key limehd:codes="89" limehd:keyLabel="Y"/>
        <Key limehd:codes="90" limehd:keyLabel="Z"/>
        <Key limehd:codes="-5" limehd:keyIcon="@drawable/sym_keyboard_delete_light" limehd:isModifier="true"
                limehd:keyWidth="15%p" limehd:keyEdgeFlags="right"
                limehd:isRepeatable="true"/>
    </Row>

    <Row limehd:rowEdgeFlags="bottom">
        <Key limehd:codes="-3"  limehd:keyIcon="@drawable/sym_keyboard_done_light" limehd:isModifier="true" limehd:keyWidth="15%p" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="-10" limehd:keyLabel="ä¸­"  limehd:isModifier="true" limehd:keyWidth="10%p" />
        <Key limehd:codes="58" limehd:keyLabel=":" limehd:keyWidth="10%p"/>
        <Key limehd:codes="32" limehd:keyIcon="@drawable/sym_flat_keyboard_space"  limehd:keyWidth="30%p" limehd:isRepeatable="true"/>
        <Key limehd:codes="59" 
        		limehd:keyLabel=";"
        		 limehd:popupKeyboard="@xml/popup_c_punctuation"   
        		limehd:keyWidth="10%p"/>
        <Key limehd:codes="-2"  limehd:keyLabel="@string/label_symbol_key" limehd:isModifier="true"/>
        <Key limehd:codes="10" limehd:keyIcon="@drawable/sym_keyboard_return_light" limehd:isModifier="true" limehd:keyWidth="15%p" limehd:keyEdgeFlags="right"/>
    </Row>
    
    
</Keyboard>
    