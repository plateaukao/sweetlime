		<Key limehd:codes="111" limehd:keyLabel="o" />
		<Key limehd:codes="112" limehd:keyLabel="p"
			limehd:keyEdgeFlags="right" />
	</Row>

	<Row>
		<Key limehd:codes="97" limehd:keyLabel="a" limehd:keyEdgeFlags="left" />
		<Key limehd:codes="115" limehd:keyLabel="s" />
		<Key limehd:codes="100" limehd:keyLabel="d" />
		<Key limehd:codes="102" limehd:keyLabel="f" />
		<Key limehd:codes="103" limehd:keyLabel="g" />
		<Key limehd:codes="104" limehd:keyLabel="h" />
		<Key limehd:codes="106" limehd:keyLabel="j" />
		<Key limehd:codes="107" limehd:keyLabel="k" />
		<Key limehd:codes="108" limehd:keyLabel="l" />
		<Key limehd:codes="59" limehd:keyLabel=";" limehd:keyEdgeFlags="right" />
