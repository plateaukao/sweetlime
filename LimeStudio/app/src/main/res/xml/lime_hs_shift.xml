	<Row>
		<Key limehd:codes="122" limehd:keyLabel="z" limehd:keyEdgeFlags="left" />
		<Key limehd:codes="120" limehd:keyLabel="x" />
		<Key limehd:codes="99" limehd:keyLabel="c" />
		<Key limehd:codes="118" limehd:keyLabel="v" />
		<Key limehd:codes="98" limehd:keyLabel="b" />
		<Key limehd:codes="110" limehd:keyLabel="n" />
		<Key limehd:codes="109" limehd:keyLabel="m" />
		<Key limehd:codes="91" limehd:keyLabel="[" />
		<Key limehd:codes="93" limehd:keyLabel="]" />
		<Key limehd:codes="-5" limehd:keyIcon="@drawable/sym_keyboard_delete_light" limehd:isModifier="true"
			limehd:isRepeatable="true" />
	</Row>


	<Row limehd:rowEdgeFlags="bottom">
		<Key limehd:codes="-3"  limehd:keyIcon="@drawable/sym_keyboard_done_light" limehd:isModifier="true"
			limehd:keyEdgeFlags="left" />
		<Key limehd:codes="-9" 
				limehd:keyLabel="EN"  limehd:isModifier="true" 
				limehd:keyWidth="10%p" 
				/>
		<Key limehd:codes="45" limehd:keyLabel="-" />
		<Key limehd:codes="61" limehd:keyLabel="=" />
		<Key limehd:codes="46" 
				limehd:keyLabel="."
				 limehd:popupKeyboard="@xml/popup_c_punctuation"   
				limehd:keyWidth="10%p" />
		<Key limehd:codes="32"
			limehd:keyIcon="@drawable/sym_flat_keyboard_space"
			limehd:keyWidth="10%p" limehd:isRepeatable="true" />
		<Key limehd:codes="44" limehd:keyLabel="," limehd:keyWidth="10%p" />
        <Key limehd:codes="39" limehd:keyLabel="\'"/>
		<Key limehd:codes="47" limehd:keyLabel="/" limehd:keyWidth="10%p" />
		<Key limehd:codes="10" limehd:keyIcon="@drawable/sym_keyboard_return_light" limehd:isModifier="true"
			limehd:keyEdgeFlags="right"/>
	</Row>
</Keyboard>