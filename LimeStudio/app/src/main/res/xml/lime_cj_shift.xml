<Keyboard xmlns:limehd="http://schemas.android.com/apk/res-auto"
    limehd:keyWidth="10%p"
    limehd:horizontalGap="0px"
    limehd:verticalGap="0px"
    limehd:keyHeight="@dimen/key_height"
    >
    
    <Row>
		<Key limehd:codes="81" limehd:keyLabel="Q\n手" limehd:keyEdgeFlags="left" />
		<Key limehd:codes="87" limehd:keyLabel="W\n田" />
		<Key limehd:codes="69" limehd:keyLabel="E\n水" />
		<Key limehd:codes="82" limehd:keyLabel="R\n口" />
		<Key limehd:codes="84" limehd:keyLabel="T\n廿" />
		<Key limehd:codes="89" limehd:keyLabel="Y\n卜" />
		<Key limehd:codes="85" limehd:keyLabel="U\n山" />
		<Key limehd:codes="73" limehd:keyLabel="I\n戈" />
		<Key limehd:codes="79" limehd:keyLabel="O\n人" />
		<Key limehd:codes="80" limehd:keyLabel="P\n心" limehd:keyEdgeFlags="right" />
	</Row>
	
    
    <Row>
        <Key limehd:codes="65" limehd:keyLabel="A\n日"
        	 limehd:horizontalGap="5%p" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="83" limehd:keyLabel="S\n尸" />
		<Key limehd:codes="68" limehd:keyLabel="D\n木" />
		<Key limehd:codes="70" limehd:keyLabel="F\n火" />
		<Key limehd:codes="71" limehd:keyLabel="G\n土" />
		<Key limehd:codes="72" limehd:keyLabel="H\n竹" />
		<Key limehd:codes="74" limehd:keyLabel="J\n十" />
		<Key limehd:codes="75" limehd:keyLabel="K\n大" />
		<Key limehd:codes="76" limehd:keyLabel="L\n中" limehd:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key limehd:codes="-1" limehd:keyIcon="@drawable/sym_keyboard_shift_shifted"  limehd:isModifier="true"
                limehd:keyWidth="15%p" 
                limehd:isSticky="true" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="90" limehd:keyLabel="Z\n重" />
		<Key limehd:codes="88" limehd:keyLabel="X\n難" />
		<Key limehd:codes="67" limehd:keyLabel="C\n金" />
		<Key limehd:codes="86" limehd:keyLabel="V\n女" />
		<Key limehd:codes="66" limehd:keyLabel="B\n月" />
		<Key limehd:codes="78" limehd:keyLabel="N\n弓" />
		<Key limehd:codes="77" limehd:keyLabel="M\n一" />
        <Key limehd:codes="-5" limehd:keyIcon="@drawable/sym_keyboard_delete_light" limehd:isModifier="true"
                limehd:keyWidth="15%p" limehd:keyEdgeFlags="right"
                limehd:isRepeatable="true"/>
    </Row>

	<Row limehd:rowEdgeFlags="bottom">
        <Key limehd:codes="-3"  limehd:keyIcon="@drawable/sym_keyboard_done_light" limehd:isModifier="true" limehd:keyWidth="15%p" limehd:keyEdgeFlags="left"/>
        <Key limehd:codes="-9" limehd:keyLabel="EN"  limehd:isModifier="true" limehd:keyWidth="10%p" />
        <Key limehd:codes="58" limehd:keyLabel=":"  limehd:keyWidth="10%p"/>
        <Key limehd:codes="32" limehd:keyIcon="@drawable/sym_flat_keyboard_space"  limehd:keyWidth="30%p" limehd:isRepeatable="true"/>
        <Key limehd:codes="59" 
        		limehd:keyLabel=";"  limehd:popupKeyboard="@xml/popup_punctuation" 
        		limehd:keyWidth="10%p"/>
        <Key limehd:codes="-2" limehd:keyLabel="123" limehd:isModifier="true" />
        <Key limehd:codes="10" limehd:keyIcon="@drawable/sym_keyboard_return_light" limehd:isModifier="true" limehd:keyWidth="15%p" limehd:keyEdgeFlags="right"/>
    </Row>
    
    
</Keyboard>
    