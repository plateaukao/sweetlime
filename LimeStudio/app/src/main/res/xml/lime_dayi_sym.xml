	<Row>
		<Key limehd:codes="-1" limehd:keyIcon="@drawable/sym_keyboard_shift" limehd:isModifier="true"
			limehd:keyWidth="15%p" limehd:isSticky="true"
			limehd:keyEdgeFlags="left" />
		<Key limehd:codes="122" limehd:keyLabel="z\n心" />
		<Key limehd:codes="120" limehd:keyLabel="x\n水" />
		<Key limehd:codes="99" limehd:keyLabel="c\n鹿" />
		<Key limehd:codes="118" limehd:keyLabel="v\n禾" />
