<Keyboard xmlns:limehd="http://schemas.android.com/apk/res-auto"
	limehd:keyWidth="10%p" limehd:horizontalGap="0px"
	limehd:verticalGap="0px" limehd:keyHeight="@dimen/key_height">

	<Row>
		<Key limehd:codes="49" limehd:keyLabel="1\n言" limehd:keyEdgeFlags="left" />
		<Key limehd:codes="50" limehd:keyLabel="2\n牛" />
		<Key limehd:codes="51" limehd:keyLabel="3\n目" />
		<Key limehd:codes="52" limehd:keyLabel="4\n四" />
		<Key limehd:codes="53" limehd:keyLabel="5\n王" />
		<Key limehd:codes="54" limehd:keyLabel="6\n門" />
		<Key limehd:codes="55" limehd:keyLabel="7\n田" />
		<Key limehd:codes="56" limehd:keyLabel="8\n米" />
		<Key limehd:codes="57" limehd:keyLabel="9\n足" />
		<Key limehd:codes="48" limehd:keyLabel="0\n金" limehd:keyEdgeFlags="right" />
	</Row>

	<Row>
		<Key limehd:codes="113" limehd:keyLabel="q\n石" limehd:keyEdgeFlags="left" />
		<Key limehd:codes="119" limehd:keyLabel="w\n山" />
		<Key limehd:codes="101" limehd:keyLabel="e\n一" />
		<Key limehd:codes="114" limehd:keyLabel="r\n工" />
