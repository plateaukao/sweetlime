	<Row>
		<Key limehd:codes="97" limehd:keyLabel="a\n人" limehd:keyEdgeFlags="left" />
		<Key limehd:codes="115" limehd:keyLabel="s\n革" />
		<Key limehd:codes="100" limehd:keyLabel="d\n日" />
		<Key limehd:codes="102" limehd:keyLabel="f\n土" />
		<Key limehd:codes="103" limehd:keyLabel="g\n手" />
		<Key limehd:codes="104" limehd:keyLabel="h\n鳥" />
		<Key limehd:codes="106" limehd:keyLabel="j\n月" />
		<Key limehd:codes="107" limehd:keyLabel="k\n立" />
